<% include header %>

<h1 class="articleTitle"><%=title%></h1>


<div class="JSLite_tabs wp">
    <ul class="JSLite_tabHead">
        <li class="on"><a href="#">编辑</a></li>
        <li><a href="#">预览</a></li>
    </ul>
    <ul class="JSLite_tabCon contact_form">
        <li class="on">
            <form action="contact" method="post">
                <input type="hidden" name="type" value="contact" />
                <div class="form-group">
                    <label for="inputTitle">标题：</label>
                    <input type="title" name="title" class="form-control" id="inputTitle" value="<%=data.title%>" placeholder="请输入标题">
                </div>
                <div class="form-group">
                    <label for="contents">内容：</label>
                    <textarea name="contents" id="contents" class="form-control" placeholder="请在这里输入内容"><%=data.contents %></textarea>
                </div>
                <input class="btn btn-default" type="submit" value="Submit" />
            </form>
        </li>
        <li id="section_preview">-

        </li>
    </ul>
</div>


<% include footer %>